{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES2022"
        ],
        "module": "ES2022",
        "moduleResolution": "bundler",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": false,
        "outDir": "../dist-electron"
    },
    "include": [
        "main.ts",
        "utils.ts"
    ],
    "exclude": [
        "node_modules",
        "preload.ts"
    ]
}